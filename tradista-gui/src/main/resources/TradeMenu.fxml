<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<HBox maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity"
	styleClass="root"
	xmlns="http://javafx.com/javafx/8.0.60"
	xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<Label fx:id="tradeType" maxHeight="-Infinity"
			maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity">
			<font>
				<Font name="System Bold" size="15.0" />
			</font>
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Label>
		<Label text="Load by trade id:">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Label>
		<TextField fx:id="load">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</TextField>
		<Button mnemonicParsing="false" onAction="#load" text="Load">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Button>
		<Button mnemonicParsing="false" onAction="#save" text="Save">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Button>
		<Button mnemonicParsing="false" onAction="#copy" text="Copy">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Button>
		<Button mnemonicParsing="false" onAction="#clear" text="New">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Button>
		<Button mnemonicParsing="false" onAction="#refresh"
			text="Refresh">
			<HBox.margin>
				<Insets right="10.0" />
			</HBox.margin>
		</Button>
	</children>
	<opaqueInsets>
		<Insets />
	</opaqueInsets>
</HBox>